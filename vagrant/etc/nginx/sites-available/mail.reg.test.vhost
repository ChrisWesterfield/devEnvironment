server {
    
    listen 443 ssl;
    server_name mail.reg.test;

    location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection upgrade;
        proxy_set_header Host $host;
        proxy_http_version 1.1;
        proxy_pass http://127.0.0.1:8025;
    }

    access_log off;
    error_log  /vagrant/log/mail.reg.test-error.log error;

    ssl_certificate     /vagrant/etc/nginx/ssl/site/mail.reg.test.crt;
    ssl_certificate_key /vagrant/etc/nginx/ssl/site/mail.reg.test.key;
}

